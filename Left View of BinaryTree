#RECURSION

def LeftView(root):
    res=[]
    
    helper(root, 0, res)
    return res
    
def helper(root, level, res):
    if root is None:
        return 
    if level== len(res):
        res.append(root.data)
        
    helper(root.left, level+1, res)
    helper(root.right, level+1, res)
    
    
