class Solution:
    def InOrder(self,root):
        if not root:
            return []
        
        stack=[]            #to level up and go to root
        res=[]
        while root or stack:
            while root:
                stack.append(root)
                root= root.left
            root = stack.pop()
            res.append(root.data)
            root= root.right
            
        return res
